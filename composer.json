{
    "name": "try-again-later/multi-backed-enum",
    "description": "A small PHP library for creating enums with cases backed by multiple values.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit --do-not-cache-result",
        "lint": "phpstan analyse -c ./phpstan.neon src tests"
    },
    "autoload": {
        "psr-4": {
            "TryAgainLater\\MultiBackedEnum\\": "src/"
        }
    },
    "authors": [
        {
            "name": "try-again-later"
        }
    ],
    "require": {
        "php": ">=8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "phpstan/phpstan": "^2.1"
    }
}
